GET         /search_notes/:name        controllers.MainController.getNotes(name: String)

POST        /new                       controllers.MainController.createNote()
GET         /update_notes/             controllers.MainController.updateNote(id: Int, password: String )
GET         /delete_notes/:id          controllers.MainController.deleteNote(id: Int)

GET         /history/all               controllers.MainController.getHistoryLogAll()
GET         /history/:noteId           controllers.MainController.getHistoryLogByNoteId(noteId: Int)

POST        /import_csv                controllers.MainController.importNotes()
GET         /export_csv                controllers.MainController.exportNotes()